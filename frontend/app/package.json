{
  "name": "@insight/app",
  "private": true,
  "version": "0.0.0-development",
  "description": "Insight app",
  "license": "MIT",
  "author": "Meemaw <ematej.snuderl@gmail.com>",
  "homepage": "https://github.com/Meemaw/insight#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Meemaw/insight.git"
  },
  "bugs": {
    "url": "https://github.com/Meemaw/insight/issues"
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "test": "jest --env=jest-environment-jsdom-sixteen",
    "test:e2e": "testcafe chrome e2e/specs",
    "lint": "concurrently \"npm run prettier\" \"npm run eslint\"",
    "eslint": "eslint --max-warnings 0 --ext .ts 'src/**/*'",
    "prettier": "prettier -l 'src/**/*'",
    "storybook": "start-storybook",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "snapshot": "build-storybook && percy-storybook --widths=320,1280",
    "pretest": "yarn workspace @insight/storybook build",
    "prebuild": "yarn workspace @insight/storybook build"
  },
  "dependencies": {
    "baseui": "^9.67.0",
    "formik": "^2.1.4",
    "ky": "^0.19.0",
    "ky-universal": "^0.5.0",
    "next": "^9.3.2",
    "next-cookies": "^2.0.3",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-hook-form": "^5.2.0",
    "styletron-engine-atomic": "^1.4.6",
    "styletron-react": "^5.2.7",
    "swr": "^0.2.0",
    "yup": "^0.28.3"
  },
  "devDependencies": {
    "@insight/storybook": "0.0.0-development",
    "@next/bundle-analyzer": "^9.3.2",
    "@testing-library/jest-dom": "^5.3.0",
    "@testing-library/testcafe": "^4.0.0",
    "@types/react": "^16.9.24",
    "@types/react-dom": "^16.9.5",
    "@types/uuid": "^7.0.2",
    "@types/yup": "^0.26.33",
    "uuid": "^7.0.2"
  }
}
